# .env.example
# Copy this file to .env and fill in your credentials

# Crunchyroll credentials
CRUNCHYROLL_EMAIL=your_email@example.com
CRUNCHYROLL_PASSWORD=your_password

# AniList OAuth credentials
# Create an app at https://anilist.co/settings/developer
ANILIST_CLIENT_ID=your_client_id
ANILIST_CLIENT_SECRET=your_client_secret

# (Optional) FlareSolverr URL for bypassing Cloudflare protection
# e.g., FLARESOLVERR_URL=http://192.168.1.5:8191
FLARESOLVERR_URL="http://192.168.1.5:8191"